# Airflow core settings
AIRFLOW__CORE__EXECUTOR=LocalExecutor
AIRFLOW__CORE__SQL_ALCHEMY_CONN=mysql+mysqldb://airflow_user:airflow_pass@mysql-container:3306/airflow_db
AIRFLOW__CORE__FERNET_KEY=your_fernet_key_here
AIRFLOW__WEBSERVER__SECRET_KEY=your_webserver_secret_key_here

# Airflow home and config (optional, override only if needed)
# AIRFLOW_HOME=/opt/airflow
# AIRFLOW_CONFIG=/opt/airflow/airflow.cfg

# Logging
AIRFLOW__CORE__LOGGING_LEVEL=INFO

# Example: Sentry integration (optional)
# AIRFLOW__SENTRY__SENTRY_DSN=

# Example: Email settings (optional)
# AIRFLOW__SMTP__SMTP_HOST=smtp.example.com
# AIRFLOW__SMTP__SMTP_STARTTLS=True
# AIRFLOW__SMTP__SMTP_SSL=False
# AIRFLOW__SMTP__SMTP_USER=your_email_user
# AIRFLOW__SMTP__SMTP_PASSWORD=your_email_password
# AIRFLOW__SMTP__SMTP_PORT=587
# AIRFLOW__SMTP__SMTP_MAIL_FROM=airflow@example.com

# Example: Airflow Variables (available as Airflow Variables in DAGs)
# AIRFLOW_VAR_MY_API_KEY=your_api_key_here
# AIRFLOW_VAR_ENV=development

# MySQL settings for the DB container (if needed)
MYSQL_ROOT_PASSWORD=your_mysql_root_password
MYSQL_DATABASE=airflow_db
MYSQL_USER=airflow_user
MYSQL_PASSWORD=airflow_pass

# Other custom environment variables for your DAGs or plugins
# MY_CUSTOM_ENV_VAR=some_value

